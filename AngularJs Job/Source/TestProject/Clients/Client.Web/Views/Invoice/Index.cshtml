                                    <td>$858.00</td>
                                </tr>*@
                                <tr ng-repeat="detail in invoice.InvoiceDetails">
                                    <td>
                                        <div class="checkbox m-b-md m-t-none">
                                            <label class="i-checks">
                                                <input type="checkbox" value="{{detail.Id}}"><i></i>
                                            </label>
                                        </div>
                                    </td>
                                    <td><input style="width:50px;text-align:center" ng-model="detail.Quantity"></td>
                                    <td><input style="width:100%" ng-model="detail.Description"></td>
                                    <td><input ng-model="detail.Price"></td>
                                    <td class="text-right">${{detail.Price * detail.Quantity | number:2}}</td>
                                </tr>
                                <tr>
                                    <td colspan="4" class="text-right"><strong>Subtotal</strong></td>
                                    <td class="text-right">${{subtotal() | number:2}}</td>
                                </tr>
                                <tr>
                                    <td colspan="4" class="text-right no-border"><strong>Shipping</strong></td>
                                    <td class="text-right">${{invoice.ShippingFee | number:2}}</td>
                                </tr>
                                <tr>
                                    <td colspan="4" class="text-right no-border"><strong>VAT Included in Total</strong></td>
                                    <td class="text-right">${{invoice.VAT | number:2}}</td>
                                </tr>
                                <tr>
                                    <td colspan="4" class="text-right no-border"><strong>Total</strong></td>
                                    <td class="text-right"><strong>${{grandTotal() | number:2}}</strong></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div>
                        <button id="btnSave" class="btn btn-sm btn-primary" ng-click="updateInvoice()">Save</button>
                        <button id="btnAddItem" class="btn btn-sm btn-primary" ng-click="addItem()">Add item</button>
                        <button id="btnDeleteItem" class="btn btn-sm btn-primary" ng-click="delete()">Delete item</button>
                    </div>
                </div>


            </div>
        </div>
